{
  "name": "website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "dll": "webpack --config webpack-dll.js",
    "dev": "NODE_ENV=development node_modules/webpack/bin/webpack.js serve --config webpack.server.js",
    "watch": "shell/check-dll.sh 'rm -rf public && NODE_ENV=development webpack --progress --color -w'",
    "build": "rm -rf public && npm run build-dice",
    "build-dice": "NODE_ENV=production webpack --progress --color",
    "tslint-fix": "tslint --fix 'src/**/*.tsx'",
    "lint:js": "tslint 'src/**/*.tsx'",
    "stylelint-fix": "stylelint 'src/**/*.scss' --config .stylelintrc.json --fix --syntax scss && npm run prettier:scss",
    "prettier:scss": "prettier -c --write \"src/**/*.scss\"",
    "release": "shell/build.sh",
    "analyze": "rm -rf public && NODE_ENV=production webpack --config webpack.analyze.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.scss": "npm run stylelint:fix",
    "src/**/*.{md,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erda-project/website.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
